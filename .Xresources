! Rofi
rofi.color-window: argb:c82d303b, #7c8389, #1d1f21
rofi.color-normal: argb:31d1f21, #c4cbd4, argb:31d1f21, #4084d6, #f9f9f9
rofi.color-urgent: argb:2c1d1f21, #cc6666, argb:e54b5160, #a54242, #f9f9f9
rofi.color-active: argb:2c1d1f21, #65acff, argb:e44b5160, #4491ed, #f9f9f9
rofi.font: Inconsolata 16
rofi.bw: 0
rofi.padding: 300
rofi.lines: 8
rofi.line-margin: 0
rofi.eh: 2
rofi.fullscreen: true
rofi.separator-style: solid
rofi.matching: fuzzy
rofi.terminal: urxvtc-256color
rofi.display-combi: $
! Urxvt

URxvt.scrollBar: false
URxvt.font: xft:inconsolata:medium:size=16
URxvt.letterSpace: -1
! URxvt.blurRadius: 20

! Mocks Mod+K behavior in iTerm2 to clear scroll buffer
! Mod4 = Super
! credit: https://superuser.com/a/1150111/59068
URxvt.keysym.C-k: command:\033c

URxvt.keysym.C-Prior: command:\033]720;1\007
URxvt.keysym.C-Next: command:\033]721;1\007
URxvt.depth: 32

URxvt.foreground: grey90
URxvt.background: rgba:0000/0000/0000/aaaa
